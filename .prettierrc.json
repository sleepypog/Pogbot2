{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "always",
    "importOrder": ["<THIRD_PARTY_MODULES>", "^[./]"],
    "importOrderParserPlugins": ["importAttributes"],
    "importOrderSeparation": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
